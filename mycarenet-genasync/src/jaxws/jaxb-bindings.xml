<?xml version="1.0" encoding="UTF-8"?>
<jxb:bindings version="2.1"
	xmlns:jxb="http://java.sun.com/xml/ns/jaxb"
	xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
	xmlns:jaxws="http://java.sun.com/xml/ns/jaxws"
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/jaxb http://java.sun.com/xml/ns/jaxb/bindingschema_2_0.xsd
	http://java.sun.com/xml/ns/jaxws http://java.sun.com/xml/ns/jaxws/wsdl_customizationschema_2_0.xsd">

	<jxb:bindings schemaLocation="../wsdl/GenericAsync-V3.xsd"
		node="/xs:schema">
		<jxb:schemaBindings>
			<jxb:package name="be.e_contract.mycarenet.genasync.jaxb">
				<jxb:javadoc><![CDATA[<body>JAXB Generic Async</body>]]></jxb:javadoc>
			</jxb:package>
		</jxb:schemaBindings>
		<jxb:bindings
			schemaLocation="../wsdl/commonTypes-v1.4.xsd" node="/xs:schema">
			<jxb:schemaBindings>
				<jxb:package
					name="be.e_contract.mycarenet.genasync.jaxb.common.types">
					<jxb:javadoc><![CDATA[<body>JAXB Common Types</body>]]></jxb:javadoc>
				</jxb:package>
			</jxb:schemaBindings>
		</jxb:bindings>
		<jxb:bindings schemaLocation="../wsdl/commonInfo-v2.xsd"
			node="/xs:schema">
			<jxb:schemaBindings>
				<jxb:package
					name="be.e_contract.mycarenet.genasync.jaxb.common.info">
					<jxb:javadoc><![CDATA[<body>JAXB Common Info</body>]]></jxb:javadoc>
				</jxb:package>
			</jxb:schemaBindings>
		</jxb:bindings>
		<jxb:bindings schemaLocation="../wsdl/xmlmime.xsd"
			node="/xs:schema">
			<jxb:schemaBindings>
				<jxb:package
					name="be.e_contract.mycarenet.genasync.jaxb.xmlmime">
					<jxb:javadoc><![CDATA[<body>JAXB XML Mime</body>]]></jxb:javadoc>
				</jxb:package>
			</jxb:schemaBindings>
		</jxb:bindings>
	</jxb:bindings>

</jxb:bindings>
